<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Person Name: <%= ID%></h1>
    <h2>Mensaje: <%= MSG%></h2>

    <% switch (MSG ) {
        case 'Hola' : %>

            <% for(var i=0; i < TM ; i++) {%>
             <p> <%=MSG %> <%=ID %></p>
             <% } %>
            <% break;
        
        case 'Adios' : %>
            <% for(var i=0; i < TM ; i++) {%>
            <p> <%=MSG %> <%=ID %></p>
            <% } %>
            <% break;
        
        case 'Bienvenido' : %>
            <% for(var i=0; i < TM ; i++) {%>
            <p> <%=MSG %> <%=ID %></p>
            <% } %>
            <% break;
        
        } %>

</body>
</html>